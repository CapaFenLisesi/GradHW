           FractionBox[
            SuperscriptBox[
             RowBox[{"(", 
              SuperscriptBox["\[Lambda]a", "\[Dagger]"], ")"}], "2"], 
            RowBox[{"2", "!"}]], "+", 
           FractionBox[
            SuperscriptBox[
             RowBox[{"(", 
              SuperscriptBox["\[Lambda]a", "\[Dagger]"], ")"}], "3"], 
            RowBox[{"3", "!"}]], "+"}], "..."}], "+", 
         FractionBox[
          SuperscriptBox[
           RowBox[{"(", 
            SuperscriptBox["\[Lambda]a", "\[Dagger]"], ")"}], "n"], 
